<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= homeStartingText %></p>
<% const reversedPosts = [...posts].reverse(); %>
<% reversedPosts.forEach( (post) => { %>
  <h1><%= post.title %></h1>
  <p><%= post.body %><a href="posts/<%= post.kebabTitle %>">Read More</a></p>
<% }); %>
<%- include("partials/footer") -%>